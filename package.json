{
  "name": "e-commerce-project",
  "version": "2.0.0",
  "description": "An Ecommerce Project in ExpressJs and MongoDb",
  "main": "server.js",
  "scripts": {
    "dev:serve:nodemon": "nodemon src/server",
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/api/server.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"nodemon dist/server.js\"",
    "ts:dev": "nodemon src/script.ts",
    "ts:build": "tsc",
    "coverage": "c8 node --test __test__",
    "lint": "eslint . --ext .ts",
    "ts:start": "tsc && node dist/server.js",
    "dev:serve": "ts-node -r tsconfig-paths/register src/server",
    "start:dev": "ts-node-dev --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest --watchAll",
    "test:dev": "jest --detectOpenHandles"
  },
